{
	"cpu": "pentium4",
	"data-layout": "e-m:e-p:32:32-f64:32:64-f80:32-n8:16:32-S128",
	"llvm-target": "i686-unknown-none",
	"target-endian": "little",
	"target-pointer-width": "32",
	"target-c-int-width": "32",
	"features": "-mmx,-sse,+soft-float",
	"os": "none",
	"arch": "x86",
	"linker-flavor": "gnu-cc",
	"linker": "i486-elf-gcc",
	"pre-link-args": {
		"gnu-cc": [
			"-Tbuild/x86/kernel/kernel.ld",
			"-nostdlib",
			"--entry=_do_start"
		]
	},
	"post-link-args": {
		"gnu-cc": [
			"src/hal/arch/x86/start.x86.o",
			"src/hal/arch/x86/hooks.x86.o"
		]
	},
	"executables": true,
	"code-model": "small",
	"exe-suffix": ".elf",
	"panic-strategy": "abort"
}
